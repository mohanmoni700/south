{
    "name": "magento/magento-cloud-template",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.4.3",
    "license": "OSL-3.0",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "xtento": {
            "type": "composer",
            "url": "https://repo.xtento.com"
        },
        "amasty": {
            "type": "composer",
            "url": "https://composer.amasty.com/enterprise/"
        },
        "magefan-blog-plus-extra-m2": {
            "type": "composer",
            "url": "https://EskhNm2ngLubrLmh7t7wvKpZDD2joFMF:uoXgzFEhOaPyoOsdag9Q8YBlesCWla2i@magefan.com/repo/"
        },
        "wyomind": {
            "type": "composer",
            "url": "https://digital_alfakher_com:Sszk1yxb@repo.wyomind.com"
        },
        "repo": {
            "type": "composer",
            "url": "https://repo.magento.com"
        },
        "private-packagist": {
            "type": "composer",
            "url": "https://corratech.repo.packagist.com/al-fakher/"
        },
        "mageplaza": {
            "type": "composer",
            "url": "https://repo.mageplaza.com"
        },
        "glew": {
            "type": "vcs",
            "url": "https://github.com/glewio/glew-magento2-service.git"
        }
    },
    "require": {
        "amasty/ajax-scroll": "^1.4",
        "amasty/base": "^1.13",
        "amasty/promo": "^2.11",
        "amasty/shopby": "^2.18",
        "auctane/api": "^2.2",
        "avalara/excise": "^1.2",
        "corra/custom-attributes-graph-ql": "^1.0",
        "corra/log": "^1.3",
        "corra/log-cloud-support": "^2.0",
        "corra/module-amasty-promo-graph-ql": "^1.1",
        "corra/module-page-builder-customisation": "^1.0",
        "corra/module-pwa-maintenance-check": "^1.0",
        "corra/module-signifyd-graph-ql": "^1.0",
        "corra/module-spreedly": "^1.1",
        "corra/module-veratad": "^1.0",
        "corra/module-yotpo-loyalty-extended": "^1.0",
        "corra/new-relic-reporting-graph-ql": "^1.0",
        "glewio/glew-magento2-service": "2.1.2",
        "klaviyo/magento2-extension": "^4.0",
        "laminas/laminas-serializer": "^2.11",
        "magefan/module-blog-extra": "^2.10.3",
        "magefan/module-blog-graph-ql": "^2.1",
        "magento/composer-root-update-plugin": "~1.1",
        "magento/extension-b2b": "^1.3",
        "magento/magento-cloud-metapackage": ">=2.4.3 <2.4.4",
        "mageplaza/magento-2-seo-extension": "^4.1",
        "mageplaza/module-delete-orders": "^4.0",
        "mageplaza/module-product-labels": "^4.0",
        "mageplaza/module-product-labels-graphql": "^1.0",
        "mageplaza/module-sitemap": "^4.0",
        "mageplaza/module-webhook": "^4.1",
        "mageworx/module-ordereditormeta": "^1.8",
        "msp/cmsimportexport": "^1.1",
        "olegkoval/magento2-regenerate-url-rewrites": "^1.6",
        "paradoxlabs/firstdata": "^2.3",
        "pcapredict/tag": "^2.1",
        "plugincompany/cmsrevisions": "^100.2",
        "quickbooks/v3-php-sdk": "^6.0",
        "signifyd/module-connect": "^4.4",
        "veratad/ageverification": "^3.3",
        "wyomind/massproductimport": "^8.6",
        "xtento/orderimport": "^2.6",
        "yotpo/magento2-module-yotpo-loyalty": "^1.2"
    },
    "replace": {
        "klevu/module-mysqlcompat": "*"
    },
    "suggest": {
        "ext-pcntl": "Need for run processes in parallel mode"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "extra": {
        "component_paths": {
            "trentrichardson/jquery-timepicker-addon": "lib/web/jquery/jquery-ui-timepicker-addon.js",
            "components/jquery": [
                "lib/web/jquery.js",
                "lib/web/jquery/jquery.min.js",
                "lib/web/jquery/jquery-migrate.js"
            ],
            "blueimp/jquery-file-upload": "lib/web/jquery/fileUploader",
            "components/jqueryui": [
                "lib/web/jquery/jquery-ui.js"
            ],
            "twbs/bootstrap": [
                "lib/web/jquery/jquery.tabs.js"
            ],
            "tinymce/tinymce": "lib/web/tiny_mce_4"
        },
        "magento-force": true,
        "magento-deploystrategy": "copy"
    },
    "scripts": {
        "generate-schema": [
            "php ./vendor/bin/ece-tools schema:generate || true"
        ],
        "post-install-cmd": [
            "@generate-schema"
        ],
        "post-update-cmd": [
            "@generate-schema"
        ]
    }
}
