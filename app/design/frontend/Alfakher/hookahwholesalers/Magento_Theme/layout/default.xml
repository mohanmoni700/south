<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
         <referenceBlock name="header.panel" remove="true"/>
         <referenceContainer name="header.panel.wrapper">
              <block class="Magento\Cms\Block\Block" name="header_links">
                <arguments>
                  <argument name="block_id" xsi:type="string">header_links</argument>
                </arguments>
              </block>
        </referenceContainer>

        <referenceContainer name="header-wrapper">
              <block class='Magento\Framework\View\Element\Template' name="reorder_link"  after="top.search" template="Magento_Theme::reorder_link.phtml"/>
        </referenceContainer>

        <referenceContainer name="header-wrapper">
              <block class='Magento\Framework\View\Element\Template' name="register_link"  after="minicart" template="Magento_Theme::register.phtml"/>
        </referenceContainer>

       <!--  <move element="register-link" destination="header-wrapper"  after="minicart"/> -->
       <move element="top.search" destination="header-wrapper" before="minicart"/>
       <move element="register_link" destination="header-wrapper" after="minicart"/>
       <move element="reorder_link" destination="header-wrapper" after="top.search"/>

